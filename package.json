{
  "name": "dweb-search-frontend",
  "version": "0.1.0",
  "description": "Frontend for dweb-search, formerly ipfs-search.",
  "author": "Kees van Drongelen <kees@visualspace.nl>",
  "scripts": {
    "serve": "vite",
    "preview": "vite preview --open --port 8080",
    "build": "vite build --out-dir dist",
    "lint": "eslint --ext .js,.mjs,.cjs,.jsx,.ts,.tsx,.vue src",
    "lint:fix": "eslint --ext .js,.mjs,.cjs,.jsx,.ts,.tsx,.vue src --fix",
    "test": "vitest watch",
    "test:once": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@cospired/i18n-iso-languages": "^3.1.1",
    "@mdi/font": "^6.5.95",
    "@vue/compat": "^3.2.37",
    "core-js": "^3.22.8",
    "howler": "^2.2.1",
    "ipfs-search-client": "^1.0.1",
    "masonry-layout": "^4.2.2",
    "mime": "^3.0.0",
    "moment": "^2.29.2",
    "regenerator-runtime": "^0.13.9",
    "ts-node": "^10.8.0",
    "video.js": "^7.12.1",
    "vite": "^2.9.9",
    "vue": "^3.2.13",
    "vue-closable": "0.0.3",
    "vue-filter-pretty-bytes": "^0.1.6",
    "vue-masonry": "^0.11.8",
    "vue-router": "^4.0.15",
    "vue-timeago": "^5.1.3",
    "vuetify": "^3.0.0-beta.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.1.0",
    "@vue/test-utils": "^2.0.0",
    "c8": "^7.11.3",
    "eslint": "8.17.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-vue": "^9.1.0",
    "jest-extended": "^2.0.0",
    "jsdom": "^19.0.0",
    "typescript": "^4.7.3",
    "vitest": "^0.13.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": "16.x"
